<html>
	<head>
		<!-- Les métas qui doivent être sur toutes les pages et qui seront toujours
				appelées -->
		<meta charset="UTF-8">
		<meta http-equiv="X-UA-Compatible" content="IE=edge">
		<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-
						fit=no">
		<title>
			Wethenew
		</title>
		{% block stylesheets %}
			{{encore_entry_link_tags('app')}}
		{% endblock %}
		{% block javascript %}
			{{encore_entry_script_tags('app')}}
		{% endblock %}

	</head>
	<body>
		{% include 'Layout/_header.html.twig' %}
		<main>
			{% include 'Layout/_flash.html.twig' %}
				{% include 'Layout/_aside.html.twig' %}
			{% block body %}

			{% endblock %}
		</main>
		{% include 'Layout/_footer.html.twig' %}
	</body>

</html>
